                                              <label class="lable" for="on-4">ON</label>
                                              <div class="content off-4">
                                              </div>
                                              <div class="content on-4 expandable <%= campData.audience_retargeting == '1' ? '' : 'd-none' %>"
                                                 data-toggle="retargetingWrapper">
                                                 <div class="row d-block my-2">
                                                    <div class="text-left">
                                                    </div>
