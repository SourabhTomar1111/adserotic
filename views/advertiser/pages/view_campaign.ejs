<!-- Begin Page Content -->
<div class="container-fluid addedit">
    <%- include('../layouts/alerts') %>
    <!-- Page Heading -->
    <div class=" align-items-center justify-content-between mb-4 pb-3 border-bottom border-dark">
       <h1 class="h3 mb-0 text-white">View Campaign</h1>       
    </div>
    <!-- ===================================== form section start =====================================-->
    <section>
       <section>
          <div class="row">
             <div class="col-md-12">
                <div id="add-campaigns">
                   <div class="hero border-dark border-1 rounded mb-5">
                      <div class="py-5 px-4 text-center">
                         <div class="row">
                            <div class="col-md-12 text-center">
                               <h6 class="text-color font-weight-bold">BASIC SETTINGS</h6>
                            </div>
                         </div>
                         <div class="text-left">
                            <div class="row mt-4 ">
                               <div class="col-md-3">
                                  <div class="form-group ">
                                     <label for="exampleFormControlSelect15" class="text-color">Campaign
                                     Name</label>
                                     <div  class="form-control"><%= campData.campaign_name; %></div>
                                  </div>
                               </div>
                            </div>
                        
                            <div class="row mt-1 ">
                               <div class="col-md-12 ">
                                  <div class="form-group ">
                                     <label for="exampleFormControlSelectg1" class="text-color">Campaign
                                     Group Name
                                     <span class="text-gray-600">(Organize your
                                     campaigns by grouping
                                     them just
                                     like folders on your computer (maximum 61
                                     groups)).</span>
                                     </label>
                                     <div class="row">
                                        <div class="col-md-3">
                                           <ul>
                                              <% if(grpData.length){ %>
                                              <% grpData.forEach(function(grp){ %>
                                              <% if(campData.group_id == grp.id){ %>
                                              <li><%= grp.name %></li>
                                              <% } %>
                                              <% }); %>
                                              <% } %>
                                           </ul>
                                        </div>
                                     </div>
                                  </div>
                               </div>
                            </div>
                            <div class="row mt-1">
                               <div class="col-md-12">
                                  <div class="form-group">
                                     <label for="exampleFormControlSelectg2" class="text-color">Labels
                                     <span class="text-gray-600"> (Find related
                                     campaigns faster by tagging
                                     them
                                     with up to six labels).</span> </label>
                                     <div class="row">
                                        <div class="col-md-3">
                                           <ul>
                                              <% if(lblData.length){ %>
                                              <% lblData.forEach(function(lbl){ %>
                                              <% if(campData.label_ids == lbl.id){ %>
                                              <li>
                                                 <%= lbl.title %>
                                              </li>
                                              <% } %>
                                              <% }); %>
                                              <% } %>
                                           </ul>
                                        </div>
                                     </div>
                                  </div>
                               </div>
                            </div>
                            <div class="row mt-1">
                               <div class="col-md-12">
                                  <div class="form-group">
                                     <label for="exampleFormControlSelectg2" class="text-color">Device
                                     <span class="text-gray-600"> (Select the device
                                     you want your ads to
                                     be
                                     displayed on).</span> </label>
                                     <div class="row">
                                        <div class="col-md-3 radio-tab">
                                           <div class="btn-group btn-group-toggle"
                                              data-toggle="buttons">
                                              <label class="btn btn-secondary active">
                                              <input type="radio" name="device_type" value="all"
                                                 id="option1" <%= campData.device_type == 'all' ? 'checked' : '' %> >
                                              All
                                              </label>
                                              <label class="btn btn-secondary">
                                              <input type="radio" name="device_type"
                                                 value="desktop" id="option2"  <%= campData.device_type == 'desktop' ? 'checked' : '' %>>
                                              Desktop
                                              </label>
                                              <label class="btn btn-secondary">
                                              <input type="radio" name="device_type"
                                                 value="mobile" id="option3"  <%= campData.device_type == 'mobile' ? 'checked' : '' %>>
                                              Mobile
                                              </label>
                                           </div>
                                        </div>
                                     </div>
                                  </div>
                               </div>
                            </div>
                            <!-- <div class="row mt-1 ">
                               <div class="col-md-12 ">
                                   <div class="form-group ">
                                       <label for="exampleFormControlSelectg1"
                                           class="text-color">Conversion
                                           Tracker
                                           <span class="text-gray-600">(Select the
                                               tracker(s) you want to add
                                               to your
                                               campaign).</span> </label>
                                       <div class="row">
                                           <div class="col-md-3">
                                               <div  class="form-control"><%= campData.tracker; %></div>
                                           </div>                                                        
                                       </div>
                                   </div>
                               </div>
                               </div> -->
                            <div class="row mt-1">
                               <div class="col-md-12">
                                  <div class="form-group">
                                     <label for="exampleFormControlSelectg2" class="text-color">Ad
                                     Format<span class="text-gray-600"> (Select the
                                     ad format your
                                     campaign will
                                     contain).</span> </label>
                                     <div class="row">
                                        <div class="col-md-12">
                                           <div class="radio-t ml-3">
                                              <input type="radio" name="ad_format" id="one" value="1"
                                                 data-toggle-target="displayAds"  <%= campData.ad_format == '1' ? 'checked' : '' %> />
                                              <label class="lable" for="one">Display</label>
                                              <input class="" type="radio" name="ad_format" id="two"
                                                 value="2" data-toggle-target="videoAds"  <%= campData.ad_format == '2' ? 'checked' : '' %> />
                                              <label class="lable" for="two">In-Stream
                                              Video</label>
                                              <p class="text-color">Ad Dimension<span
                                                 class="text-gray-600">
                                                 (Select the size of the ads your
                                                 campaign will
                                                 contain).</span>
                                              </p>
                                              <div class="content one" data-toggle="displayAds"
                                                 data-toggle-group="ad_format">
                                                 <div class="form-group">
                                                    <div class="row">
                                                       <div class="col-md-3">
                                                          <% if(campData.ad_dimension_size){ %>
                                                          <div class="form-control">
                                                             <%= campData.ad_dimension_size %>
                                                          </div>
                                                          <% } %>                                                                                
                                                       </div>
                                                    </div>
                                                 </div>
                                              </div>
                                              <div class="content two d-none" data-toggle="videoAds"
                                                 data-toggle-group="ad_format">
                                                 <div class="row">
                                                    <div class="col-md-3">
                                                       <p class="bg-light p-2 rounded">Video
                                                          needs to be 16:9
                                                          Ratio
                                                       </p>
                                                    </div>
                                                 </div>
                                              </div>
                                           </div>
                                        </div>
                                     </div>
                                  </div>
                               </div>
                            </div>
                         </div>
                      </div>
                   </div>
                </div>
             </div>
          </div>
       </section>
       <!-- ===================================== form --1 End =======================================-->
       <section>
          <div class="row">
             <div class="col-md-12">
                <div id="add-campaigns">
                   <div class="hero border-dark border-1 rounded mb-5">
                      <div class="py-5 px-4 text-center">
                         <div class="row">
                            <div class="col-md-12 text-center">
                               <h6 class="text-color font-weight-bold">SCHEDULE</h6>
                            </div>
                         </div>
                         <div class="text-left">
                            <div class="row mt-1">
                               <div class="col-md-12">
                                  <div class="form-group">
                                     <label class="text-color">Duration <span class="text-gray-600">
                                     (Define
                                     your
                                     campaign start and end dates).</span>
                                     </label>
                                     <div class="row">
                                        <div class="col-md-12">
                                           <div class="radio-b ml-3">
                                              <input type="radio" name="duration" value="0" id="off-1"
                                                 <%= campData.duration == '0' ? 'checked' : '' %>
                                                 data-toggle-target="durationWrapper" />
                                              <label class="lable" for="off-1">OFF</label>
                                              <input class="" type="radio" name="duration" value="1" id="on-1"
                                                 data-toggle-target="durationWrapper" <%= campData.duration == '1' ? 'checked' : '' %> />
                                              <label class="lable" for="on-1">ON</label>
                                              <div class="content off-1">
                                              </div>
                                              <div class="content on-1 expandable <%= campData.duration == '1' ? '' : 'd-none' %>"
                                                 data-toggle="durationWrapper">
                                                 <div class="row">
                                                    <div class="col-md-12">
                                                       <div class="text-left">
                                                          <div class="row ">
                                                             <div class="col-md-10">
                                                                <div class="row d-block">
                                                                   <div class="col-md-9">
                                                                      <div
                                                                         class="form-group ">
                                                                         <label
                                                                            for="exampleFormControlSelect001"
                                                                            class="text-color">Start
                                                                         date</label>
                                                                         <% if(campData.ad_start_at){ %>
                                                                         <div class="form-control">
                                                                            <%= campData.ad_start_at %>
                                                                         </div>
                                                                         <% } %>      
                                                                      </div>
                                                                   </div>
                                                                   <div class="col-md-9">
                                                                      <div
                                                                         class="form-group ">
                                                                         <label
                                                                            for="exampleFormControlSelect001"
                                                                            class="text-color">End
                                                                         date</label>
                                                                         <% if(campData.ad_start_at){ %>
                                                                         <div class="form-control">
                                                                            <%= campData.ad_end_at %>
                                                                         </div>
                                                                         <% } %>    
                                                                      </div>
                                                                   </div>
                                                                </div>
                                                             </div>
                                                          </div>
                                                       </div>
                                                    </div>
                                                 </div>
                                              </div>
                                           </div>
                                        </div>
                                     </div>
                                  </div>
                               </div>
                            </div>
                            <hr class="hr">
                         </div>
                         <div class="text-left">
                            <div class="row mt-1">
                               <div class="col-md-12 ">
                                  <div class="form-group">
                                     <label class="text-color">Frequency Capping <span
                                        class="text-gray-600">
                                     (Set
                                     how many times a visitor will see your
                                     campaign over a
                                     designated time
                                     unit).</span> </label>
                                     <div class="row">
                                        <div class="col-md-12">
                                           <div class="radio-b ml-3">
                                              <input type="radio" name="ad_frequency_capping"
                                                 value="0" id="off-2" <%= campData.frequency_capping == '0' ? 'checked' : '' %>
                                                 data-toggle-target="frequencyWrapper" />
                                              <label class="lable" for="off-2">OFF</label>
                                              <input class="" type="radio" name="ad_frequency_capping"
                                                 value="1" id="on-2" <%= campData.frequency_capping == '1' ? 'checked' : '' %>
                                                 data-toggle-target="frequencyWrapper" />
                                              <label class="lable" for="on-2">ON</label>
                                              <div class="content off-2">
                                              </div>
                                              <div class="content on-2  expandable <%= campData.frequency_capping == '1' ? '' : 'd-none' %>"
                                                 data-toggle="frequencyWrapper">
                                                 <div class="row d-block">
                                                    <div class="text-left">
                                                       <div class="col-md-2">
                                                          <div class="form-group time-in">
                                                             <% if(campData.frequency_cap_times){ %>
                                                             <div class="form-control">
                                                                <%= campData.frequency_cap_times %>
                                                             </div>
                                                             <% } %>    
                                                          </div>
                                                       </div>
                                                       <div class="col-md-3">
                                                          <p class="text-color">
                                                             time(s) to a visitor
                                                             every
                                                          </p>
                                                       </div>
                                                       <div class="col-md-2">
                                                          <div class="form-group time-in">
                                                             <% if(campData.frequency_cap_every){ %>
                                                             <div class="form-control">
                                                                <%= campData.frequency_cap_every %>
                                                             </div>
                                                             <% } %>  
                                                          </div>
                                                       </div>
                                                       <div class="col-md-3">
                                                          <% if(campData.frequency_cap_units){ %>
                                                          <div class="form-control">
                                                             <%= campData.frequency_cap_units %>
                                                          </div>
                                                          <% } %>  
                                                       </div>
                                                    </div>
                                                 </div>
                                              </div>
                                           </div>
                                        </div>
                                     </div>
                                  </div>
                               </div>
                            </div>
                            <hr class="hr">
                         </div>
                      </div>
                   </div>
                </div>
             </div>
          </div>
       </section>
       <!-- ===================================== form --2 End  =====================================-->
       <section>
          <div class="row">
             <div class="col-md-12">
                <div id="add-campaigns">
                   <div class="hero border-dark border-1 rounded mb-5">
                      <div class="py-5 px-4 text-center">
                         <div class="row">
                            <div class="col-md-12 text-center">
                               <h6 class="text-color font-weight-bold">AUDIENCE</h6>
                            </div>
                         </div>
                         <div class="text-left">
                            <div class="row mt-1">
                               <div class="col-md-12">
                                  <div class="form-group">
                                     <label for="exampleFormControlSelectg2" class="text-color">Content
                                     Category
                                     <span class="text-gray-600"> ( Select the
                                     content preference of the
                                     audience
                                     you would like to target).</span> </label>
                                     <div class="row">
                                        <div class="col-md-3 radio-tab">
                                           <div class="btn-group btn-group-toggle"
                                              data-toggle="buttons">
                                              <label class="btn btn-secondary active">
                                              <input type="radio" name="category" value="straight" id="option01"
                                                 <%= campData.category == 'straight' ? 'checked' : '' %>>
                                              Straight
                                              </label>
                                              <label class="btn btn-secondary">
                                              <input type="radio" name="category" value="gay" id="option02" <%= campData.category == 'gay' ? 'checked' : '' %>>
                                              Gay
                                              </label>
                                              <label class="btn btn-secondary">
                                              <input type="radio" name="category" value="trans" id="option03" <%= campData.category == 'trans' ? 'checked' : '' %>>
                                              Trans
                                              </label>
                                           </div>
                                        </div>
                                     </div>
                                  </div>
                               </div>
                            </div>
                            <hr class="hr">
                            <div class="row mt-1">
                               <div class="col-md-12">
                                  <div class="form-group">
                                     <label for="exampleFormControlSelectg2"
                                        class="text-color">Demographic
                                     Targeting
                                     <span class="text-gray-600"> ( Select the
                                     demographic you would like
                                     to
                                     target.
                                     By default All genders will be
                                     targeted).</span> </label>
                                     <div class="row">
                                        <div class="col-md-3 radio-tab">
                                           <p class="mt-2"> Gender</p>
                                           <div class="btn-group btn-group-toggle"
                                              data-toggle="buttons">
                                              <label class="btn btn-secondary active">
                                              <input type="radio" name="gender" value="all" id="option001"
                                                 <%= campData.gender == 'all' ? 'checked' : '' %>> All
                                              </label>
                                              <label class="btn btn-secondary">
                                              <input type="radio" name="gender" value="male" id="option002" 
                                                 <%= campData.gender == 'male' ? 'checked' : '' %>>
                                              Male
                                              </label>
                                              <label class="btn btn-secondary">
                                              <input type="radio" name="gender" value="female" id="option003"
                                                 <%= campData.gender == 'female' ? 'checked' : '' %>                                                     >
                                              Female
                                              </label>
                                           </div>
                                        </div>
                                     </div>
                                  </div>
                               </div>
                            </div>
                            <hr class="hr">
                         </div>
                         <div class="text-left">
                            <div class="row mt-1">
                               <div class="col-md-12">
                                  <div class="form-group">
                                     <label class="text-color">Audience Exclusion <span
                                        class="text-gray-600">
                                     (Prevent users who have already purchased
                                     your product from
                                     seeing your
                                     ads for this campaign).</span> </label>
                                     <p class="mt-2"> Pixel Targeting</p>
                                     <div class="row ">
                                        <div class="col-md-12">
                                           <div class="radio-b ml-3">
                                              <input type="radio" name="audience_exclusion"
                                                 value="0" id="off-3" <%= campData.audience_exclusion == '0' ? 'checked' : '' %>  
                                                 data-toggle-target="audienceExclusionWrapper" />
                                              <label class="lable" for="off-3">OFF</label>
                                              <input class="" type="radio" name="audience_exclusion"
                                                 value="1" id="on-3" <%= campData.audience_exclusion == '1' ? 'checked' : '' %>  
                                                 data-toggle-target="audienceExclusionWrapper" />
                                              <label class="lable" for="on-3">ON</label>
                                              <div class="content off-3">
                                              </div>
                                              <div class="content on-3 expandable <%= campData.audience_exclusion == '1' ? '' : 'd-none' %>"
