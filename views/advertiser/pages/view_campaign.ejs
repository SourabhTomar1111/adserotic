                                                    <div class="col-md-12">
                                                       <div id="hero">
                                                          <div
                                                             class="hero border-dark border-1 rounded">
                                                             <div class="py-5 px-5 ">
                                                                <div class="table-responsive">
                                                                   <table class="table">
                                                                      <thead>
                                                                         <tr
                                                                            class="table-primary-color">
                                                                            <th scope="col">
                                                                               Site
                                                                               Name
                                                                            </th>
                                                                            <th scope="col">
                                                                               Cookie
                                                                               Name
                                                                            </th>
                                                                            <th scope="col">
                                                                               Cookie
                                                                               Value
                                                                            </th>
                                                                            <th scope="col">
                                                                               Cookie
                                                                               Age
                                                                            </th>
                                                                            <th scope="col">
                                                                               Invocation
                                                                               Code
                                                                            </th>
                                                                         </tr>
                                                                      </thead>
                                                                      <tbody>
