                                                                         <% }); %>
                                                                         <% } %>
                                                                      </tbody>
                                                                   </table>
                                                                </div>
                                                             </div>
                                                          </div>
                                                       </div>
                                                    </div>
                                                 </div>
                                              </div>
                                           </div>
                                        </div>
                                     </div>
                                  </div>
                               </div>
                            </div>
                            <hr class="hr">
                         </div>
                      </div>
                   </div>
                </div>
             </div>
          </div>
       </section>
       <!-- ===================================== form --3 End =======================================-->
       <section>
          <div class="row">
             <div class="col-md-12">
                <div id="add-campaigns">
                   <div class="hero border-dark border-1 rounded mb-5">
                      <div class="py-5 px-4 text-center">
                         <div class="row">
                            <div class="col-md-12 text-center">
                               <h6 class="text-color font-weight-bold">TARGETING</h6>
                            </div>
                         </div>
                         <div class="text-left">
                            <div class="row mt-1">
                               <div class="col-md-12">
                                  <div class="form-group">
                                     <label class="text-color">Keyword Targeting <span
                                        class="text-gray-600">
                                     (Select
                                     up to 30 target and 20 excluded keywords to
                                     target specific
                                     content , a
                                     maximum of 4 broad are allowed).</span>
                                     </label>
                                     <div class="row">
                                        <div class="col-md-12">
                                           <div class="radio-b ml-3">
                                              <input type="radio" name="keywordTarget" id="off-5"
                                                 <%= campData.keyword_targeting == '0' ? 'checked' : '' %> value="0"
                                                 data-toggle-target="keywordWrapper" />
                                              <label class="lable" for="off-5">OFF</label>
                                              <input class="" type="radio" name="keywordTarget"
                                                 <%= campData.keyword_targeting == '1' ? 'checked' : '' %> value="1" id="on-5"
                                                 data-toggle-target="keywordWrapper" />
                                              <label class="lable" for="on-5">ON</label>
